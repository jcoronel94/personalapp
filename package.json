    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "axios": "^0.21.0",
    "buttercms": "^1.2.3",
    "core-js": "^3.6.5",
    "emailjs-com": "^2.6.3",
    "vue": "^2.6.12",
    "vue-breakpoint": "^2.1.0",
    "vue-carousel": "^0.18.0",
    "vue-material": "^1.0.0-beta-15",
    "vue-router": "^3.4.7",
    "vuelidate": "^0.7.6",
    "vuex": "^3.5.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.3",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
